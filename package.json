  "license": "MIT",
  "dependencies": {
    "amqplib": "=0.4.1",
    "debug": "^2.2.0"
  },
  "repository": "git@github.com:1602/transport-rabbit.git",
  "devDependencies": {
    "expect": "^1.20.1",
    "mocha": "^2.5.3",
    "istanbul": "^0.4.4",
    "coveralls": "^2.11.11"
  }
}
